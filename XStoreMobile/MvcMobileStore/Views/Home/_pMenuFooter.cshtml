@model IEnumerable<MvcMobileStore.Models.Menu>


@*--Scrip Menu_Footer--*@
<script type="text/javascript">
    $(function () {
        // effect sub menu
        var $actived = $("#sub-nav ul.sub-menu li.active");
       
    })
</script>

<nav id="sub-nav">
    <ul class="sub-menu ce clearfix">
        @foreach (var item in Model)
        {
            if (item.Thutu == 1)
            {
            <li class="btn-home fl active">
                <a title="@item.TenMenu" href="@item.url">
                    <span></span>
                </a>
            </li>
            }
            else
            {
            <li class="item fl">
                <a href="@item.url">@item.TenMenu</a>
            </li>
            }
        }

        <li class="back-top"></li>
    </ul>
</nav>
